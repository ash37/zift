<div id="<%= dom_id unavailability_request %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">User:</strong>
    <%= unavailability_request.user_id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Starts at:</strong>
    <%= unavailability_request.starts_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Ends at:</strong>
    <%= unavailability_request.ends_at %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Reason:</strong>
    <%= unavailability_request.reason %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= unavailability_request.status %>
  </div>
</div>
